<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
<constant name="struts.devMode" value="true"/>
<package name="com.internousdev.sample1.action" extends="struts-default">

<action name="LoginAction" class="com.internousdev.sample1.action.LoginAction" method="execute">
<result name="success">home.jsp</result>
</action>

<action name="GoHomeAction" class="com.internousdev.sample1.action.GoHomeAction" method="execute">
<result name="success">home.jsp</result>
</action>

<action name="WelcomeAction" class="com.internousdev.sample1.action.WelcomeAction" method="execute">
<result name="success">login.jsp</result>
</action>

<action name="UserCreateAction" class="com.internousdev.sample1.action.UserCreateAction" method="execute">
<result name="success">userCreate.jsp</result>
</action>

<action name="MyPageAction" class="com.internousdev.sample1.action.MyPageAction" method="execute">
<result name="success">myPage.jsp</result>
<result name="error">myPageError.jsp</result>
</action>

<action name="UserCreateConfirmAction" class="com.internousdev.sample1.action.UserCreateConfirmAction" method="execute">
<result name="success">userCreateConfirm.jsp</result>
<result name="error">userCreateError.jsp</result>
</action>

<action name="UserCreateCompleteAction" class="com.internousdev.sample1.action.UserCreateCompleteAction" method="execute">
<result name="success">userCreateComplete.jsp</result>
</action>

<action name="CartAction" class="com.internousdev.sample1.action.CartAction" method="execute">
<result name="success">cart.jsp</result>
</action>

<action name="BuyItemAction" class="com.internousdev.sample1.action.BuyItemAction" method="execute">
<result name="success">buyItemConfirm.jsp</result>
<result name="error">home.jsp</result>
</action>

<action name="BuyItemConfirmAction" class="com.internousdev.sample1.action.BuyItemConfirmAction" method="execute">
<result name="success">buyItemComplete.jsp</result>
<result name="error">home.jsp</result>
</action>


</package>
</struts>